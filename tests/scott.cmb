### Scott's pentaminos problem #############################################
### we eliminate the symetries of the problem by using only one
### orientation of pentamino F

include "pentaminos.cmb"

pattern scott_board = {
********
********
********
***..***
***..***
********
********
********
}

pattern scott_board3 =
  set set set set (constant 8x8 true) 3x3 false 3x4 false 4x3 false 4x4 false

problem scott_problem = scott_board3
[ L ~one ~sym, T ~one ~sym, V ~one ~sym, N ~one ~sym,
  Z ~one ~sym, F ~one     , X ~one ~sym, W ~one ~sym,
  P ~one ~sym, I ~one ~sym, Y ~one ~sym, U ~one ~sym ]

print scott_problem

timing on

solve dlx scott_problem svg "scott.svg"

count dlx scott_problem

solve sat "minisat2" scott_problem svg "scott2.svg"


