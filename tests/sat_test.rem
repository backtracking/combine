
### Scott's pentaminos problem #############################################
### we eliminate the symetries of the problem by using only one
### orientation of pentamino F

include "pentaminos.rem"

pattern board = {
********
********
********
***.****
***..***
********
********
********
}

problem sat_problem = board
[ L ~one ~sym, T ~one ~sym, V ~one ~sym, N ~one ~sym,
  Z ~one ~sym, F ~one     , X ~one ~sym, W ~one ~sym,
  P ~one ~sym, I ~one ~sym, Y ~one ~sym, U ~one ~sym ]


#print scott_problem

timing on
count dlx sat_problem

solve dlx sat_problem svg "test.svg"

answer_to_the_question_of_life_the_universe_and_everything

dimacs sat_problem "test.dimacs"


exit



